                {
                    "name": "hac",
                    "contextPath": "/hac"
                },
                {
                    "name": "mediaweb",
                    "contextPath": "/medias"
                },
                {
                    "name": "backoffice",
                    "contextPath": ""
                },
                {
                    "name": "oauth2",
                    "contextPath": "/oauth2"
                },
                {
                    "name": "ycommercewebservices",
                    "contextPath": "/ycommercewebservices"
                }
            ]
        }
