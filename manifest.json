{
    "name": "cc-spartacus",
    "commerceSuiteVersion": "6.7.0.1",
    "extensions": [
        "backoffice",
        "platformbackoffice",
        "apparelstore",
        "electronicsstore",
        "modeltacceleratorservices",
        "ycommercewebservices",
        "cmsoccaddon",
        "yacceleratorcore",
        "yacceleratorinitialdata",
        "yacceleratorfacades",
        "yacceleratorstorefront"
    ],

    "storefrontAddons": [
        {
            "addon": "cmsoccaddon",
            "storefront": "ycommercewebservices",
            "template": "ycommercewebservices"
        }
    ],
    "aspects": [
        {
            "name": "backoffice",
            "webapps": [
                {
                    "name": "hac",
                    "contextPath": "/hac"
                },
                {
                    "name": "mediaweb",
                    "contextPath": "/medias"
                },
                {
                    "name": "backoffice",
                    "contextPath": "/backoffice"
                },
                {
                    "name": "oauth2",
                    "contextPath": "/oauth2"
                },
                {
                    "name": "ycommercewebservices",
                    "contextPath": ""
                }
            ]
        }
    ],
    "tests": {
        "extensions": ["ycommercewebservices"],
        "annotations": ["UnitTests", "IntegrationTests"],
        "packages": ["de.hybris.platform.*"]
    },
    "webTests": {
        "extensions": ["ycommercewebservices"],
        "excludedPackages": ["de.hybris.platform.*"]
    },
    "skipOrchestrationTests": "true"
}
